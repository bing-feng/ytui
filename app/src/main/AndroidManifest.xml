<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.winton.ytui">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ytui"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ytui"
        android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.pages.ButtonsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pages.IconsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pages.TabsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pages.BarsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pages.ProfileActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>